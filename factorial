package javaprogram;
import java.util.Scanner;

public class Factoral {
	
	
	
		public static void main(String args[])
		{
		
		Scanner s=new Scanner(System.in);
		System.out.println("enter the number");
		int num=s.nextInt();
		int fact=1;
		for(int i=1;i<=num;i++) {
			fact=fact*i;
			
		}
		
		System.out.println("factorial"+num+"is"+fact);
	}
	}
