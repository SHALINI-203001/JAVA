package javaprogram;
import java.util.*;

public class Palin {
	public static void main(String args[])
	{
		int n;
		
		Scanner sc=new Scanner(System.in);
		System.out.println("enter a number");
		n=sc.nextInt();
		int temp=n;
		int rev=0,rem;
		while(temp!=0)
		{
			rem=temp%10;
			rev=rev*10+rem;
			temp=temp/10;
		}
		if(n==rev) {
			System.out.println("number is palindrome");
			
		}
		else
		{
			System.out.println("number is not palindrome");
		}
;
		
	}

}
